.title{
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: #AB956D;
  width: 100%;
  font-size: 26rpx;
  letter-spacing: 1px;
}
.bottom_tip{
    height: 60rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #333;
    font-size: 28rpx;
}

.placeholder{
    color: #666;
    font-size: 24rpx;
}

/* 滑到底部的样式 */
.hua-to-bottom{
    width: 100%;
    height: 60rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #ccc;
    font-size: 24rpx;
}
.disabled{
    pointer-events: none;
}
